{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build onenote2md solution",
			"type": "shell",
			"command": "msbuild \"Onenote2md.Solution\\onenote2md solution.sln\" /t:Build /p:Configuration=Debug",
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build"
		},
		{
			"label": "Build onenote2md solution (dotnet)",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"Onenote2md.Solution/onenote2md solution.sln",
				"-c",
				"Debug"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}